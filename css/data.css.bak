#content{  min-height:700px; margin-top:0;  position:static;  background-color:#fff; 
padding: 131px 0 50px 0; }


#content h2{position:absolute; left:9999%; width:1px; height:1px; overflow:hidden; font-size:0; line-height:0;}
#content h3{font-size:50px; font-weight:normal; text-align:center; padding:70px 0; 
	letter-spacing:5px; color:#0f94e1; border-bottom:5px solid #0f94e1; margin-bottom:10px;}
#content .breadInner{width:100%;  border-top:1px solid #ddd;  border-bottom:1px solid #ddd; background-color:#f1f1f1;}
#content .breadcrumb{width:1000px; margin:0 auto;  height:40px;}
#content .breadcrumb li{display:inline; padding:10px 20px; border-right:1px solid #ddd; }
#content .breadcrumb .gohome{border-left:1px solid #ddd; vertical-align:middle;}
#content .breadcrumb .gohome img{width:20px;}
#content .breadcrumb li a{line-height:40px; width:100%; height:100%; font-weight:bold; font-size:13px;}
#content .breadcrumb .breadDown img{width:10px; margin-left:5px; vertical-align:middle;}

#content .subnav {
    overflow: hidden;
    margin-top: -105px;
    margin-bottom: 75px;
}
#content .subnavL a, #content .subnavR a{color:#666;}

#content .subnavL{ display:inline-block;  float:left;  width:100px; height:30px;  }
#content .subnavL a{display:block; width:100%; height:100%; padding-left:30px; 
	background:#fff url("../image/subnav_arr01.png") no-repeat left center;}
#content .subnavR{ display:inline-block;  float:right;  width:100px; height:30px; }
#content .subnavR a{display:block; width:100%; height:100%;  
 background: #fff url("../image/subnav_arr02.png") no-repeat right center; text-indent:3em;}




#content .dataInner1{width:1000px; margin:0 auto  ; }

#content .dataSample{width:100%; height:100%;}
#content .dataSample img{width:100%; }


#content .dataInner1 strong, #content .dataInner1 span{display:block;}
#content .dataInner1 .dataListTop strong{ font-size:20px; margin-bottom:10px;}
#content .dataInner1 .dataListTop span{font-size:12px; margin-top:10px;}

#content .dataInner2{width:1020px; margin:0 auto 50px ; }
#content .dataInner2 strong, #content .dataInner2 span{display:block;}


#content .dataList{overflow:hidden; width:100%; }
#content .dataListTop{width:100%; overflow:hidden; margin-top:20px;}
#content .dataBoxTop1, #content .dataBoxTop2{width:50%; float:left; }



#content .dataBoxTop1 dd p{padding: 40px 5%; margin-right:2%; 
 border:1px solid #eee; }
#content .dataBoxTop2 dd p{ padding:40px 5%; margin-left:2%; 
	 border:1px solid #eee;  }

#content .dataBoxTop1 a {display:block; width:98%; height:490px; margin-right:2%; 
	background: url("../image/info_166.jpg")  no-repeat top center ;  background-size:100%;
	border-bottom:40px solid #f1f1f1; }

#content .dataBoxTop2 a {display:block; width:98%; height:490px; margin-left:2%; 
	background: #f1f1f1 url("../image/info_151.jpg")  no-repeat top center ;  background-size:100%;
	border-bottom:40px solid #f1f1f1;  }


#content .dataListBot{width:100%; overflow:hidden;  }
#content .dataBox{width:25%; float:left; margin-top:20px; }

#content .dataBox dt a {display:block; width:92%; height:190px; margin:0 4%; 
	background: #fff no-repeat  top center ;  background-size:100%; } 

#content .dataBox1 dt a {
	background-image:  url("../image/info_148.jpg");} 

#content .dataBox2 dt a {
background-image:  url("../image/info_147.jpg"); } 

