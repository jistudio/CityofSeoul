	background-image:  url("../image/info_164.jpg");} 

#content .dataBox5 dt a {
	background-image:  url("../image/info_153.jpg"); } 

#content .dataBox6 dt a {
	background-image:  url("../image/info_154.jpg");} 

#content .dataBox7 dt a {
	background-image:  url("../image/info_155.jpg");} 

#content .dataBox8 dt a {
	background-image:  url("../image/info_162.jpg"); } 

#content .dataBox9 dt a {
	background-image:  url("../image/info_148.jpg"); } 

#content .dataBox10 dt a {
	background-image:  url("../image/info_147.jpg");} 

#content .dataBox11 dt a {
	background-image:  url("../image/info_150.jpg");} 

#content .dataBox12 dt a {
	background-image:  url("../image/info_164.jpg"); } 


#content .dataBox strong{ padding: 20px 5%; margin: 0 4%; 
 border:1px solid #eee; }

#content .pageNum{ overflow:hidden; width:252px; margin:0 auto 50px; text-align:center;}
#content .pageNum a{border:1px solid #d6d6d6; display:block; float:left; width:34px; height:34px; line-height:34px; margin:0 3px; font-size:13px; font-weight:bold; }
#content .pageNum img{width:34px; height:34px;}
#content .pageNum a.pageDefault{background-color:#0f94e1; color:white;}