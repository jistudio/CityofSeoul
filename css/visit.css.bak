#content{  min-height:700px; margin-top:0;  position:static;  background-color:#fff; 
padding: 131px 0 50px 0; }


#content h2{position:absolute; left:9999%; width:1px; height:1px; overflow:hidden; font-size:0; line-height:0;}
#content h3{font-size:50px; font-weight:normal; text-align:center; padding:70px; 
	letter-spacing:5px; color:#0f94e1; border-bottom:5px solid #0f94e1; margin-bottom:10px;}
#content .breadInner{width:100%;  border-top:1px solid #ddd;  border-bottom:1px solid #ddd; background-color:#f1f1f1;}
#content .breadcrumb{width:1000px; margin:0 auto;  height:40px;}
#content .breadcrumb li{display:inline; padding:10px 20px; border-right:1px solid #ddd; }
#content .breadcrumb .gohome{border-left:1px solid #ddd; vertical-align:middle;}
#content .breadcrumb .gohome img{width:20px;}
#content .breadcrumb li a{line-height:40px; width:100%; height:100%; font-weight:bold; font-size:13px;}
#content .breadcrumb .breadDown img{width:10px; margin-left:5px; vertical-align:middle;}

#content .subnav {
    overflow: hidden;
    margin-top: -105px;
    margin-bottom: 75px;
}
#content .subnavL{ display:inline-block;  float:left;  width:100px; height:30px;  }
#content .subnavL a{display:block; width:100%; height:100%; padding-left:30px; color:#666;
	background:#fff url("../image/subnav_arr01.png") no-repeat left center;}
#content .subnavR{ display:inline-block;  float:right;  width:100px; height:30px; }
#content .subnavR a{display:block; width:100%; height:100%; color:#666;
 background: #fff url("../image/subnav_arr02.png") no-repeat right center; text-indent:3em;}

 .visitInner{width:1000px; margin:0 auto 50px ; }
 .visitInner h4{font-size:22px; text-indent:1em; border-left:5px solid #0f94e1; 
	  padding-bottom: 5px; margin-bottom:15px;}
 .visitInner .mapWrapper{width:100%; overflow:hidden;}
 .visitInner .mapWrapper table{width:50%;   border-collapse:collapse;  float:left; }

table caption, .googleMap h4{width:1px; height:1px; 
		position:absolute; left:-9999%; font-size:0; line-height:0; overflow:hidden; }

 .mapWrapper{width:100%; overflow:hidden; margin-bottom:5px;}

 .mapWrapper .hallPic{width:50%; float:left;}
 .mapWrapper .hallPic img{width:100%;}
 #map{width:50%; float:left; height: 500px;}

 .visitInner .googleMap img{width:100%;}

 .visitInner table.drivingInfo{width:100%;   border-collapse:collapse;  }
 .visitInner table .tableF{border-top:1px solid black;}
 .visitInner table th{text-align:left; background-color:#f1f1f1; }
 .visitInner table th,  .visitInner table td{padding:15px 0; text-indent:3em; border-bottom:1px solid #ddd;}
 .visitInner table.drivingInfo th.busIcon{padding:0px; line-height:40px;}
 .visitInner table.drivingInfo .busIcon img{width:11%; vertical-align:middle; margin-right:10px;}
 .discount{ border-left:1px solid #ddd;}

 .parking{margin-top:80px;}
 .parkingWrapper{width:100%; overflow:hidden; margin-bottom:50px;}

 .parkingWrapper ul{width:50%; float:left; }
 .parkingWrapper ul li{padding:8px 30px 7px 0; }


 .parkingWrapper p{width:50%; float:left;}
 .parkingWrapper p img{width:100%;}
 .parking table{ border-collapse:collapse; width:100%; } 
 .hallView{width:100%; margin-top:50px;}
 .hallView ul{overflow:hidden;}
 .hallView ul li{width:25%; float:left; }
 .hallView ul li img{width:99%;}

