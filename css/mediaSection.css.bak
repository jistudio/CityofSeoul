	/*1. mediaquery 쓸때 css맨뒤에 붙임 (앞에 쓰면 로딩속도 드려짐) 
	2. [space] 과 [;] 주의
	3. 디바이스별로 고치는부분만 첨가 
	4. 바뀌는 거 없는 디바이스들도 남겨둠, 지우면 안됨, 에러남
	5. min-width에 맞춘다*/



	@media all and ( min-width:1600px ){ 
		body{ }
		
}
	@media all and ( min-width:1023px ) and ( max-width:1599px ){
		body{  }
}

	@media all and ( min-width:768px ) and ( max-width:1022px ){ 
		body{ }
		#content{  padding:0px 0 50px 0;}

		#content .cultureInner {
			width: 90%;}

		#content .subnav {

			margin-top: -100px;
			margin-bottom: 70px;
		}
		#content h3 {
			font-size: 50px;
				
				padding: 60px;
			
			}
	
		#content .cultureSample {
			
			margin-bottom: -10px;
		}
	
	#content .a1, #content .a2{width:100%; float:none;}

	#content .wrapperTop dl a {
   
	  padding: 10px 0 40px;
	}


	#content .a1 a, #content .a2 a {
    margin: 0;
	
	}
	.cultureInner .a2 h4 {
		margin-left: 0;
	}
	#content article {
	 margin-bottom: 30px;
	}




}

	@media all and ( min-width:480px ) and ( max-width:767px ){ 
		body{ }
	#content{  padding:0px 0 50px 0;}

		#content .cultureInner {
			width: 90%;}
	
		
		#content h3 {
				font-size:35px;
				
				padding: 40px;
			
			}
		#content .subnav {
		margin-top: -75px;
		margin-bottom: 45px;
		}

	
		#content .cultureSample {
			
			margin-bottom: -5px;
			}
		#content .storyBox{width:100%; margin-right:0%; margin-top:10px;  float:none;}
		#content .storyBox dd{padding:5%;}
		
		#content .a1, #content .a2{width:100%; float:none;}

	#content .wrapperTop dl a {
   
	  padding: 10px 0 10px;
	}
	#content .a1 a, #content .a2 a {
    margin: 0;
	
	}
	.cultureInner .a2 h4 {
		margin-left: 0;
	}
	#content article {
	 margin-bottom: 30px;
	}




}
	@media all and ( max-width:479px ){ 

		body{}
		#content{  padding:0px 0 50px 0;}
		#wrapper{min-width:320px;}

		#content .cultureInner {
			width: 90%;}

		#content .subnav {
			display:none;
		}

		#content h3 { font-size: 30px; padding: 35px; letter-spacing:0;}

		#content .cultureSample {
			
			margin-bottom: -5px;
			}
		#content .storyBox{width:100%; margin-right:0%; margin-top:10px;  float:none;}
		#content .storyBox dd{padding:4%; font-size:90%;}

	#content .a1, #content .a2{width:100%; float:none;}

	#content .wrapperTop dl a {
   
	  padding: 10px 0 10px;
	}
	#content .a1 a, #content .a2 a {
    margin: 0;
	
	}
	.cultureInner .a2 h4 {
		margin-left: 0;
	}
	#content article {
	 margin-bottom: 30px;
	}
	#content .wrapperTop dt {
   
		margin-right: 10px;
	}
	#content .wrapperTop dd {
		 margin-left: 0px;
	}


}
