/*  ---------------------  메인 sliding START---------------------   */

.sliding{width:100%;  margin:0 auto; }/* .prev와 next의 position:absolute 기준*/
.tunnel{width:100%;  overflow:hidden;} /* .train을 숨겨줌(mask)*/
.train{width:400%;  }
.train li{width:25%;  float:left; position:relative; }

.train li img{width:100%;}


/* .sm_mv_text 의 position:absolute기준,  
움직이므로 .sliding보다는 li 에 기준을 둠  */






 .train li .sm_mv_text{position:absolute; top: 40px; left: 50%; margin-left: -490px; }

 .train li.trainbox1 .sm_mv_text{color:white; }
 .train li.trainbox2 .sm_mv_text{color:#072a56; }
 .train li.trainbox3 .sm_mv_text{color:#0f94e1; }
 .train li.trainbox4 .sm_mv_text{color:#523f41; }



	


.train li .sm_mv_text span, .train li .sm_mv_text strong{display:block;}
.train li .sm_mv_text strong{ font-size:18px; font-weight:bold; 
letter-spacing:3px; margin-bottom:10px; padding-left:5px;}



.train li .sm_mv_text span.visualTitle{font-size:80px; line-height:100px; font-family:"NanumM","맑은 고딕", "Malgun Gothic", "돋움", 'dotum'; }
.train li .sm_mv_text span.visualDesc{font-size:22px; line-height:34px; margin-top:0px;}

.train li .sm_mv_text a{display:block; width:150px; height:50px; text-align:center; 
	line-height:50px; color:white; border:1px solid white; margin-top:30px; font-weight:bold;
	font-size:14px; letter-spacing:2px;}

.train li.trainbox1 .sm_mv_text a{color:white; border:1px solid white;}
.train li.trainbox2 .sm_mv_text a{color:#072a56; border:1px solid #072a56;}
.train li.trainbox3 .sm_mv_text a{color:#0f94e1; border:1px solid #0f94e1;}
.train li.trainbox4 .sm_mv_text a{color:#523f41; border:1px solid #523f41;}



.sliding img{width:100%;}

.prev{position:absolute; top:38%; left:2%; width:64px; height:64px;}
.next{ position:absolute; top:38%; right:2%; width:64px; height:64px; }
.prev img, .next img{width:100%;}

.sliderControl{position:absolute;     top: 45px; left:50%;  margin-left:360px;}

.button {
    display: inline-block;
}
