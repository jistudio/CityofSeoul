#popWrap{width:300px; padding:50px; text-align:center; 
	position:fixed; top:0; left:0; background-color:rgba(0,0,0, 0.8); color:white;
	font-size:15px;
	z-index:3; }

.todayclose a{color:white; display:inline-block; width:70px; height:25px; background-color:#c30029; margin-left:10px; text-align:center; line-height:25px; font-size:12px;letter-spacing:1px; }
#popWrap h3{  cursor:move; font-size:30px; letter-spacing:3px;  color:white; font-weight:normal;  padding-bottom:25px; border-bottom:5px solid #0f94e1; }
#popWrap h3 img{margin-right:10px;}
#popWrap .qrwrap{overflow:hidden; margin-bottom:20px;  border-bottom:1px solid white; padding:30px 0;}

