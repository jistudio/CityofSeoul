.playStop a{ display:inline-block; width:14px; height:14px; border-radius:50%; 
	 text-align:center;  font-weight:bold; border:2px solid #c30029; 
	background:#c30029 url("../image/stopPlay.png") no-repeat -1px 0px;  }





.button li{ display:inline-block; width:15px; height:15px;  text-indent:-9999px; 
